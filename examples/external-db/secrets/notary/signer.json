{
    "server": {
        "grpc_addr": ":7899",
        "tls_cert_file": "/etc/ssl/notary/tls.crt",
        "tls_key_file": "/etc/ssl/notary/tls.key"
    },
    "logging": {
        "level": "info"
    },
    "storage": {
        "backend": "postgres",
        "db_url": "postgres://%YOUR_DB_USER%:%YOUR_DB_PASSWORD%@%YOUR_DB_HOSTNAME%:%YOUR_DB_PORT%/notarysigner?sslmode=disable",
        "default_alias": "defaultalias"
    }
}
